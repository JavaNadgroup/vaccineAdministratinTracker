<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>welcome.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>HealthCenterAdministration</servlet-name>
        <servlet-class>org.health.health.HealthCenterAdministration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>org.health.administration.AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientForm</servlet-name>
        <servlet-class>org.health.administration.PatientForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientLoginForm</servlet-name>
        <servlet-class>org.health.administration.PatientLoginForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Booking</servlet-name>
        <servlet-class>org.health.booking.Booking</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HealthCenterAdministration</servlet-name>
        <url-pattern>/HealthCenterAdministration</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>VaccinationInventory</servlet-name>
        <servlet-class>org.health.vaccine.VaccinationInventory</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VaccinationInventory</servlet-name>
        <url-pattern>/VaccinationInventory</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>VaccineAdministration</servlet-name>
        <servlet-class>org.health.administration.VaccineAdministration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VaccineAdministration</servlet-name>
        <url-pattern>/VaccineAdministration</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>VaccinationBooking</servlet-name>
        <servlet-class>org.health.booking.VaccinationBooking</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VaccinationBooking</servlet-name>
        <url-pattern>/VaccinationBooking</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SystemsAdministration</servlet-name>
        <servlet-class>org.health.administration.SystemsAdministration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SystemsAdministration</servlet-name>
        <url-pattern>/SystemsAdministration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientForm</servlet-name>
        <url-pattern>/PatientForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientLoginForm</servlet-name>
        <url-pattern>/PatientLoginForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Booking</servlet-name>
        <url-pattern>/Booking</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/AdminReg.tld</tag-uri>
            <tag-location>D:/vaccinationTracker/web/WEB-INF/tlds/AdminReg.tld</tag-location>
        </taglib>
    </tag-config>
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/AdminLogin.tld</tag-uri>
            <tag-location>D:/vaccinationTracker/web/WEB-INF/tlds/AdminLogin.tld</tag-location>
        </taglib>
    </tag-config>
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/HealthCenter_to_db.tld</tag-uri>
            <tag-location>D:/vaccinationTracker/web/WEB-INF/tlds/HealthCenter_to_db.tld</tag-location>
        </taglib>
    </tag-config>
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/patient_to_db.tld</tag-uri>
            <tag-location>D:/vaccinationTracker/web/WEB-INF/tlds/patient_to_db.tld</tag-location>
        </taglib>
    </tag-config>
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/PatientAuth.tld</tag-uri>
            <tag-location>D:/vaccinationTracker/web/WEB-INF/tlds/PatientAuth.tld</tag-location>
        </taglib>
    </tag-config>
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/No_of_patients.tld</tag-uri>
            <tag-location>D:/vaccinationTracker/web/WEB-INF/tlds/No_of_patients.tld</tag-location>
        </taglib>
    </tag-config>
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/Bookings_to_database.tld</tag-uri>
            <tag-location>D:/vaccinationTracker/web/WEB-INF/tlds/Bookings_to_database.tld</tag-location>
        </taglib>
    </tag-config>
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/No_of_bookings.tld</tag-uri>
            <tag-location>D:/vaccinationTracker/web/WEB-INF/tlds/No_of_bookings.tld</tag-location>
        </taglib>
    </tag-config>
    <tag-config>
        <taglib>
            <tag-uri>/WEB-INF/tlds/No_of_admins.tld</tag-uri>
            <tag-location>D:/vaccinationTracker/web/WEB-INF/tlds/No_of_admins.tld</tag-location>
        </taglib>
    </tag-config>
</web-app>
